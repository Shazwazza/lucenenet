      },
      {
        "files": ["**.md", "**.yml"],
        "src": "../src/dotnet/tools/lucene-cli/docs",
        "dest": "cli"
      },
      {
        "files": ["**/package.md","**/overview.md"],
        "src": "../src"
      },
      {
        "files": ["toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": [
          "logo/favicon.ico",
          "logo/lucene-net-icon-64x64.png"
        ],
        "src": "../branding"
      }
    ],   
    "globalMetadata": {
      "_appTitle": "Apache Lucene.NET 4.8.0 Documentation",      
      "_disableContribution": false,
      "_appFaviconPath": "logo/favicon.ico",
      "_enableSearch": true,
      "_appLogoPath": "logo/lucene-net-icon-64x64.png",
      "_appFooter": "Copyright Â© 2017 Licensed to the Apache Software Foundation (ASF)"
    },
    "overwrite": [
      {
        "files": ["apispec/**.md"]
      },
      {
        "files": ["**/package.md","**/overview.md"],
        "src": "../src"
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "lucenetemplate"
    ],
